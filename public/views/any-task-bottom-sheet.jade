md-bottom-sheet.md-grid(layout="row" controller="AnyTasksBottomSheetController")
  md-list(flex layout="row" layout-align="center center")
    md-list-item(ng-repeat="item in items")
      md-button.md-grid-item-content(ng-click="menu_click($index)")
        md-icon(md-font-icon="material-icons") {{ item.icon }}
        .md-grid-text {{ item.name }}
